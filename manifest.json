{
  "manifest_version": 3,
  "name": "Crates2Lib",
  "description": "Auto-Redirects crates.io links to lib.rs",
  "version": "1.0.0",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "declarativeNetRequestWithHostAccess"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["*://crates.io/*"]
}
